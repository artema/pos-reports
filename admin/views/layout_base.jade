- var is_production = false && settings.env === 'production';

doctype html
html(lang='en')
  head
    meta(name='viewport',content='width=device-width, initial-scale=1.0')
    meta(charset='utf-8')
    link(href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.4/css/bootstrap.min.css',rel='stylesheet')
    link(href='/public/libs/css/sb-admin-2.css',rel='stylesheet')
    link(href='/public/libs/css/timeline.css',rel='stylesheet')
    link(href='/public/dist/styles' + (is_production ? '.min' : '') + '.css',rel='stylesheet')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.4/js/bootstrap.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular-animate.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular-route.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular-resource.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular-sanitize.min.js')
    script(src='/public/dist/shared' + (is_production ? '.min' : '') + '.js')
    block head
  body
    div(ng-controller='ModalController')
    block body
